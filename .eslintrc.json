{
    "env": {
        "browser": true,
        "es2021": true
    },
           "extends": ["airbnb",
            "plugin:react/recommended",
            "prettier", "prettier/react" ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prefer-stateless-function": "off",
        "semi": ["error", "always"],
        "quotes": ["error", "double"]


    }
}
